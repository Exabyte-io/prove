        "prettier": "^2.7.1",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "typescript": "^4.9.5",
        "vite": "^6.0.7",
        "vite-plugin-node-polyfills": "^0.25.0"
    },
    "engines": {
        "node": ">=20.0.0",
        "npm": ">=10.0.0"
    },
    "lint-staged": {
        "*.{js,jsx,ts,tsx,!dist/**/*}": "eslint --cache --fix",
        "*.{js,jsx,ts,tsx,css,!dist/**/*}": "prettier --write"
    }
